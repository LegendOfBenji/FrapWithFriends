<!DOCTYPE html>
<html>
  <head>
    <title>FrapWithFriends</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script type="text/javascript">
      window.frapLogo = "<%= image_url("FrapLogo.jpg") %>";
      window.splashImg = "<%= image_url("FrapSplash.jpg") %>";
      window.splash1 = "<%= image_url("Splash1.png") %>";
      window.splash2 = "<%= image_url("Splash2.png") %>";
      window.splash3 = "<%= image_url("Splash3.png") %>";
    </script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    
    <link href="https://fonts.googleapis.com/css?family=Grenze&display=swap" rel="stylesheet">
  </head>

  <body>
    <%= yield %>
  </body>
</html>
